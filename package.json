{
	"name": "linear-discord-middleware",
	"version": "1.0.0",
	"description": "A Node.js middleware for Linear webhooks to Discord notifications.",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"postinstall": "pnpm build",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"linear",
		"discord",
		"webhook",
		"middleware",
		"nodejs"
	],
	"license": "MIT",
	"dependencies": {
		"axios": "^1.10.0",
		"body-parser": "^2.2.0",
		"dotenv": "^17.2.0",
		"express": "^5.1.0",
		"ts-node": "^10.9.2",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@types/body-parser": "^1.19.6",
		"@types/express": "^5.0.3",
		"@types/node": "^24.0.15",
		"nodemon": "^3.1.10",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=24"
	},
	"packageManager": "pnpm@10.13.1"
}
